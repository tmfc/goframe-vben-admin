#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules/unbuild/dist/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules/unbuild/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules/unbuild/dist/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules/unbuild/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/unbuild@3.6.1_sass@1.94.0_typescript@5.9.3_vue-tsc@3.1.4_typescript@5.9.3__vue@3.5.24_typescript@5.9.3_/node_modules:/Users/william/work/my_opensource/goframe-vben-admin/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../unbuild/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../unbuild/dist/cli.mjs" "$@"
fi
